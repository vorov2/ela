open list

class Monoid a where
  mempty a
  mappend a->a->a
  mconcat a->a

instance Monoid where
  mconcat xss = foldr mappend mempty $ xss

instance Monoid List where
  mempty = []
  mappend = (++)
  mconcat xss = [x \\ xs <- xss, x <- xs]

