fromLiteral {eq} = 
  let tup = ('E', 'l', 'a') in
    eq (length tup) 3 
  `seq` eq (tup:0) 'E' 
  `seq` eq (tup:1) 'l'
  `seq` eq (tup:2) 'a'

fromLiteral2 {eq} =
  let tup = ("Ela",) in
  eq (length tup) 1 `seq` eq (tup:0) "Ela"

equals {eqt} =
    eqt (t1 == t2)
  `seq` eqt (t1 <> t3)
  `seq` eqt (t2 <> t3)
  `seq` eqt (t3 == (3,4))
        where t1 = (1,2)
              t2 = (1,2)
              t3 = (3,4)

compare {eqt,eqf} =
    eqf (t1 > t2)
  `seq` eqt (t1 < t2)
  `seq` eqt (t1 <= t2)
  `seq` eqt (t3 > t2)
  `seq` eqt (t3 >= t2)
  `seq` eqt (t2 >= (3,4))    
        where t1 = (1,2)
              t2 = (3,4)
              t3 = (5,6)

simpleMath {eq} =
    eq ((1,2)+(2,3)) (3,5)
  `seq` eq ((4,5)-(2,1)) (2,4)
  
unary {eq} =
    eq (negate (2,1)) (-2,-1)
  `seq` eq (negate (21,4.2)) (-21,-4.2)

succPred {eq} =
    eq (succ (1,2)) (2,3)
  `seq` eq (succ (42, 'b')) (43, 'c')
  `seq` eq (pred (3,4)) (2,3)
  `seq` eq (pred (12,'B')) (11, 'A')
    
concat {eq,eqt} =
    eq (t1++t2) (1,2,3,4,5,6)
  `seq` eq (t2++t1) (4,5,6,1,2,3)
  where t1 = (1,2,3)
        t2 = (4,5,6)

lengthTest {eq} = 
    eq (length t1) 3
  `seq` eq (length t2) 1
  where t1 = (1,2,3)
        t2 = (1,)

fstSndTest {eq} =
    eq (fst t) 1 
  `seq` eq (snd t) 2
  `seq` eq (fst3 t2) 10
  `seq` eq (snd3 t2) 20
  where t = (1,2)
        t2 = (10,20,30)

typeCheck {eqt} =
  let tup  = (0,) in
  eqt (tup is Tuple)
